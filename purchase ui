local library = loadstring(game.HttpGet(game, "https://pastebin.com/raw/d2jRTQCG"))()

local w1 = library:Window("discord 01_vortex_10") -- main label


w1:Button(
    "buy sauce?[5 stomps]",
    function()
fireclickdetector(workspace.MAP.Pads["[Sauce] - 5 Stomps"].ClickDetector, 1000000000)
    end
) -- Text, Callback
--


w1:Button(
    "buy super armor?[10 stomps]",
    function()
fireclickdetector(workspace.MAP.Pads["[Super Armor] - 10 Stomps"].ClickDetector, 100000000)
    end
) -- Text, Callback
--


w1:Button(
    "buy super pizza?[5 stomps]",
    function()
fireclickdetector(workspace.MAP.Pads["[Super Pizza] - 5 Stomps"].ClickDetector, 1000000000000)
    end
) -- Text, Callback
--

w1:Button(
    "buy armor?",
    function()
 fireclickdetector(workspace.MAP.Pads:GetChildren()[11].ClickDetector, 100000000)
    end
) -- Text, Callback

w1:Button(
    "buy pizza?",
    function()
fireclickdetector(workspace.MAP.Pads["[Pizza]"].ClickDetector, 1000000000)
    end
) -- Text, Callback

